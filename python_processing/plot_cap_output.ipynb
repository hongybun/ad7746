   "source": [
    "# Data Collection\n",
    "## Setup"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "c9fe1d37",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "['COM1', 'COM3', 'COM5']\n"
     ]
    }
   ],
   "source": [
    "import serial\n",
    "import sys\n",
    "import glob\n",
    "import re\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from IPython.display import display, clear_output\n",
    "from pathlib import Path\n",
    "import datetime\n",
    "\n",
    "def serial_ports():\n",
    "    \"\"\" Lists serial port names\n",
    "\n",
    "        :raises EnvironmentError:\n",
    "            On unsupported or unknown platforms\n",
    "        :returns:\n",
    "            A list of the serial ports available on the system\n",
    "    \"\"\"\n",
    "    if sys.platform.startswith('win'):\n",
    "        ports = ['COM%s' % (i + 1) for i in range(256)]\n",
    "    elif sys.platform.startswith('linux') or sys.platform.startswith('cygwin'):\n",
    "        # this excludes your current terminal \"/dev/tty\"\n",
    "        ports = glob.glob('/dev/tty[A-Za-z]*')\n",
    "    elif sys.platform.startswith('darwin'):\n",
    "        ports = glob.glob('/dev/tty.*')\n",
    "    else:\n",
    "        raise EnvironmentError('Unsupported platform')\n",
    "\n",
    "    result = []\n",
    "    for port in ports:\n",
    "        try:\n",
    "            s = serial.Serial(port)\n",
    "            s.close()\n",
    "            result.append(port)\n",
    "        except (OSError, serial.SerialException):\n",
    "            pass\n",
    "    return result\n",
    "\n",
    "print(serial_ports())"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "a5e7a3a4",
   "metadata": {},
   "source": [
    "Select COM port in the cell below"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "c5fda129",
   "metadata": {},
   "outputs": [],
   "source": [
    "# >>>>>>>>> User Input <<<<<<<<<\n",
    "com_port = \"COM3\"\n",
    "timeout = 5\n",
    "default_array_size = 1e6\n",
    "data_base_path = Path(\"C:/Users/iSenSys/Downloads\")\n",
    "\n",
    "# Get connection\n",
    "ser = serial.Serial(com_port, timeout=timeout)\n",
    "\n",
    "# Compile regex\n",
    "# re_pattern = re.compile(r\"(?P<time>\\d+.\\d+) seconds (?P<cap>\\d+.\\d+) pF\")  # < UNCOMMENT me when C code changed\n",
    "re_pattern = re.compile(r\"Capacitance: (?P<cap>\\d+.\\d+) pF\")"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "ac49114a",
   "metadata": {},
   "source": [
    "## Collect"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "6319d692",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Check for expected initialization value\n",
    "line = ser.readline()\n",
    "\n",
    "# if line != b\"Initializing\\r\\n\":\n",
    "#     raise SystemError(f\"Unexpected response: {line}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "0b509bad",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 2000x400 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Create a figure\n",
    "fig, ax = plt.subplots(figsize=(10, 2), dpi=200)\n",
    "ax.set_xlabel(\"Time\")\n",
    "ax.set_ylabel(\"Capacitance (pF)\")\n",
    "\n",
    "# Preallocate array memory\n",
    "array = np.empty((int(default_array_size), 1))\n",
    "array[:,:] = np.nan\n",
    "\n",
    "# Collect data\n",
    "try:\n",
    "    idx = 0\n",
    "    while True:\n",
    "        # Collect data and extract values\n",
    "        line = ser.readline()\n",
    "        match = re_pattern.match(line.decode('utf-8'))\n",
    "\n",
    "        # Store data\n",
    "        array[idx, 0] = float(match.group(\"cap\"))\n",
    "        idx += 1\n",
    "\n",
    "        # Plot it like its hot\n",
    "        ax.plot(array[:, 0], color=\"blue\")  # CHANGE ME: add x value for time\n",
    "        display(fig)\n",
    "        clear_output(wait=True)\n",
    "\n",
    "except KeyboardInterrupt:\n",
    "    pass # expected -> what happens when you stop the cell"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "4891024a",
   "metadata": {},
   "source": [
    "## Save Data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "5b8c6e9b",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Create dataframe\n",
    "data_df = pd.DataFrame(array, columns=['cap'])  # CHANGE ME: to include time\n",
    "data_df = data_df.dropna()\n",
    "\n",
    "# Construct file name\n",
    "now_time = datetime.datetime.now().isoformat(timespec=\"seconds\")\n",
    "now_time = now_time.replace(\":\", \"-\")\n",
    "data_file_name = \"example_cap_data_\" + now_time + \".csv\"\n",
    "\n",
    "# Save\n",
    "data_df.to_csv(data_base_path / data_file_name)"
   ]
  },
